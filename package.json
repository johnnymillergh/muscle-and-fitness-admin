{
  "name": "muscle-and-fitness-admin",
  "version": "0.0.1-beta",
  "license": "Apache-2.0",
  "description": "Muscle and Fitness Admin (M&F)",
  "author": {
    "name": "Johnny Miller (锺俊)",
    "email": "johnnysviva@outlook.com",
    "url": "https://github.com/johnnymillergh"
  },
  "bugs": {
    "url": "https://github.com/johnnymillergh/muscle-and-fitness-admin/issues/new",
    "email": "johnnysviva@outlook.com"
  },
  "repository": {
    "url": "https://github.com/johnnymillergh/muscle-and-fitness-admin",
    "type": "git"
  },
  "keywords": [
    "admin",
    "vue",
    "typescript",
    "element-ui"
  ],
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "svg": "vsvg -s src/assets/svg-icons/svg-resouces -t src/assets/svg-icons/components --ext ts --es6",
    "build:dev_dkr": "vue-cli-service build --mode development_docker && docker-compose -f docker-compose.development_docker.yml up -d",
    "build:test": "vue-cli-service build --mode test && docker-compose -f docker-compose.test.yml up -d",
    "build:stage": "vue-cli-service build --mode stage && docker-compose -f docker-compose.stage.yml up -d",
    "build:prod": "vue-cli-service build && docker-compose -f docker-compose.production.yml up -d",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint --ext .ts,.js,.vue src",
    "go:travis": "npm run lint && npm run test:unit && npm run build:prod"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "axios-mock-adapter": "1.17.0",
    "class-validator": "^0.11.0",
    "core-js": "^3.6.4",
    "element-ui": "^2.13.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.1.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/js-cookie": "^2.2.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.0",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-plugin-router": "^4.2.3",
    "@vue/cli-plugin-typescript": "^4.2.3",
    "@vue/cli-plugin-unit-jest": "^4.2.3",
    "@vue/cli-plugin-vuex": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "@vue/eslint-config-standard": "^5.1.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "1.0.0-beta.31",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-plugin-vue-libs": "^4.0.0",
    "lint-staged": "^10.0.8",
    "node-sass": "^4.12.0",
    "sass": "^1.26.2",
    "sass-loader": "^8.0.2",
    "style-resources-loader": "^1.3.3",
    "typescript": "^3.8.3",
    "vue-cli-plugin-style-resources-loader": "^0.1.4",
    "vue-svgicon": "^3.2.6",
    "vue-template-compiler": "^2.6.11",
    "vuex-module-decorators": "^0.16.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
